# channels to use when looking for packages
channels:
  - appliedai
  - mickc
  - defaults
  - kaybenleroll


# binstar.org upload when package built locally
binstar_upload: True

# when creating new environments add these packages by default
create_default_packages:
  - python
  - jupyter
  - jupyter_console
  - numpy
  - scipy
  - matplotlib
  - pandas
  - pip


# disallowed specification names
disallow:
  - anaconda

# show channel URLs when displaying what is going to be downloaded
show_channel_urls: True
